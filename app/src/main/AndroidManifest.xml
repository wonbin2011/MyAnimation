<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wonbin.myanimation" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ExplodeAnimationActivity"
            android:label="@string/title_activity_explode_animation" >
        </activity>
        <activity
            android:name=".activities.SlideAnimationAvtivity"
            android:label="@string/title_activity_slide_animation_avtivity" >
        </activity>
        <activity
            android:name=".activities.FadeAnimationActivity"
            android:label="@string/title_activity_fade_animation" >
        </activity>
        <activity
            android:name=".activities.CircularRevealActivity"
            android:label="@string/title_activity_circular_reveal" >
        </activity>
        <activity
            android:name=".activities.SharedElementAnimationActivity"
            android:label="@string/title_activity_shared_element_animation" >
        </activity>
        <activity
            android:name=".activities.SharedElementAnimationSecondActivity"
            android:label="@string/title_activity_shared_element_aniamtion_second" >
        </activity>
    </application>

</manifest>
